{
  "name": "ordinals-marketplace",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "pretest": "npx scrypt-cli compile",
    "build:contract": "npx scrypt-cli compile",
    "deploy:contract": "npx ts-node ./scripts/deploy.ts",
    "verify:contract": "npx scrypt-cli verify $(cat .scriptHash) ./src/contracts/ordinalsMarketplace.ts",
    "genprivkey": "npx ts-node ./scripts/privateKey.ts"
  },
  "dependencies": {
    "next": "14.0.1",
    "react": "^18",
    "react-dom": "^18",
    "scrypt-ts": "^1.3.17"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "dotenv": "^16.3.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.1",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}